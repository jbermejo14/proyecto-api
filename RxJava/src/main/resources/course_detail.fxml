  fx:controller="com.svalero.RxJava.controllers.CourseDetailController"
  spacing="10" alignment="CENTER" style="-fx-padding: 20;">

    <Label text="Course Details" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

